{
  "name" : "fav-kyou",
  "description" : "twitter favorite handler chrome plugin",
  "version" : "1.1",
  "permissions": ["http://twitter.com/*", "https://twitter.com/*"],
  "manifest_version" : 2,
  "icons": {
    "16": "assert/icon16.png",
    "48": "assert/icon48.png",
    "128": "assert/icon128.png"
  },
  "content_scripts" : [
    {
      "matches" : [
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "js" : [
        "lib/jquery-2.1.3.min.js",
        "src/helper.js"
      ]
    }
  ]
}
